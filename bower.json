        "sweetalert"        : ["**/!(lib)"],
        "filesaver"         : ["**/!(FileSaver.min.js)"],
        "marionette"        : ["**/!(lib)"],
        "backgrid"          : ["**/!(lib)"],
        "moment"            : ["**/!(moment.js)"],
